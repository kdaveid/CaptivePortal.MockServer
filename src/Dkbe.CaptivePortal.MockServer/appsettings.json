{
  "AppSettings": {
    "CaptivePortalUrl": "http://localhost:5000",
    "GuestAuthSettings": {
      "LoginPage": "{zone}/signin",
      "SessionExpirationPage": "{zone}/sessionexpired",
      "IdleTimeOutPage": "{zone}/idletimeout",
      "MaxSessionsPage": "{zone}/maxsession",
      "TrafficExceededPage": "{zone}/trafficexceeded"
    },
    "StaticZoneSettings": {
      "Zones": [
        {
          "Name": "Zone-Uno",
          "LocalPath": "Zone1"
        },
        {
          "Name": "Zone-Due",
          "LocalPath": "Zone2"
        },
        {
          "Name": "Public-Zone",
          "LocalPath": "Public"
        }
      ]
    }
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
